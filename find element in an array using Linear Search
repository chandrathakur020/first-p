import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        
        Scanner scan=new Scanner(System.in);
        System.out.println("Enter the size of Array");
        int n=scan.nextInt();
        int ar[]=new int[n];
        
        int i;
        int key;
         System.out.println("Enter the Element to be formed");
         key=scan.nextInt();
        for(i=0;i<n;i++){
             System.out.println("Enter the Element");
              ar[i]=scan.nextInt();
        
             
        }
        for (i=0;i<n;i++){
            if (ar[i]==key)
        break;
    }
        if (i==n)
         System.out.println("Element not found" + key);
         else
          System.out.println("Element found at index "+key+" at "+i);
         
        
}
}
